{
  "db_name": "PostgreSQL",
  "query": "\n            UPDATE requests\n            SET\n                state = 'claimed',\n                daemon_id = $1,\n                claimed_at = $2\n            WHERE id IN (\n                SELECT id\n                FROM requests\n                WHERE state = 'pending'\n                    AND (not_before IS NULL OR not_before <= $2)\n                ORDER BY created_at ASC\n                LIMIT $3\n                FOR UPDATE SKIP LOCKED\n            )\n            RETURNING id, batch_id, template_id, endpoint, method, path, body, model, api_key, retry_attempt\n            ",
