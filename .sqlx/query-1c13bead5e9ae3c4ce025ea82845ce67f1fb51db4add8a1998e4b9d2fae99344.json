{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT\n                b.id as batch_id,\n                b.file_id,\n                f.name as file_name,\n                b.total_requests,\n                b.pending_requests,\n                b.in_progress_requests,\n                b.completed_requests,\n                b.failed_requests,\n                b.canceled_requests,\n                b.requests_started_at as started_at,\n                b.requests_last_updated_at as last_updated_at,\n                b.created_at\n            FROM batches b\n            JOIN files f ON f.id = b.file_id\n            WHERE b.id = $1\n            ",
